{
  "name": "project-janatha",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace @janatha/backend dev\" \"yarn workspace @janatha/frontend dev\"",
    "build": "yarn workspaces run build",
    "backend": "yarn workspace @janatha/backend dev",
    "frontend": "yarn workspace @janatha/frontend dev",
    "deploy": "bash infrastructure/scripts/deploy.sh"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
