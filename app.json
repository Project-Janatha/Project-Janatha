{
  "expo": {
    "name": "expo-router-example",
    "slug": "expo-router-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "fonts": {
    "family": "AnekLatin",
        "fonts": [
          { "asset": "./assets/fonts/AnekLatin-Thin.ttf", "weight": "100" },
          { "asset": "./assets/fonts/AnekLatin-ExtraLight.ttf", "weight": "200" },
          { "asset": "./assets/fonts/AnekLatin-Light.ttf", "weight": "300" },
          { "asset": "./assets/fonts/AnekLatin-Regular.ttf", "weight": "400" },
          { "asset": "./assets/fonts/AnekLatin-Medium.ttf", "weight": "500" },
          { "asset": "./assets/fonts/AnekLatin-SemiBold.ttf", "weight": "600" },
          { "asset": "./assets/fonts/AnekLatin-Bold.ttf", "weight": "700" },
          { "asset": "./assets/fonts/AnekLatin-ExtraBold.ttf", "weight": "800" }
        ]
      },
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
