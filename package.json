{
  "name": "expo-router",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "private": true,
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "check:tamagui": "tamagui check",
    "start": "npm install yarn tamagui expo; npm install && yarn install; expo start -c",
    "start:backend": "npm install yarn bcryptjs express express-session @seald-io/nedb; npm install && yarn install; node ./backend/centralSequence.js",
    "start:all": "concurrently \"yarn start:backend\" \"yarn start\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-native/js-polyfills": "^0.81.1",
    "@react-navigation/native": "^7.1.8",
    "@rnmapbox/maps": "^10.1.43",
    "@seald-io/nedb": "^4.1.2",
    "@tamagui/config": "^1.132.24",
    "@tamagui/lucide-icons": "^1.132.24",
    "@tamagui/toast": "^1.132.24",
    "@teovilla/react-native-web-maps": "^0.9.5",
    "babel-preset-expo": "~54.0.1",
    "bcryptjs": "^3.0.2",
    "burnt": "^0.13.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "expo": "^54.0.0",
    "expo-build-properties": "~1.0.8",
    "expo-cli": "^6.3.10",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "^19.0.7",
    "expo-router": "~6.0.4",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "fs": "^0.0.1-security",
    "mapbox-gl": "^2.15.0",
    "metro": "^0.83.1",
